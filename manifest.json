{
    "name": "Blazing Bookmarks", 
    "version": "0.0.9",
    "description": "Speed up your browsing with your most used bookmarks always on deck",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "storage"
  ],
  "options_page": "html/options.html",
  "action": {
    "default_icon": {
      "48": "icons/bb48.png",
      "128": "icons/bb128.png"
    },
    "default_popup": "html/popup.html"
  },
  "icons": {
    "48": "icons/bb48.png",
    "128": "icons/bb128.png"
  }
}